// Retail Rocket GTM Template
// Author: Roman Koch

// Libs
const injectScript = require('injectScript');
const createQueue = require('createQueue');
const callInWindow = require('callInWindow');
const setInWindow = require('setInWindow');

const rrInit = () => {
  // Constants
  const rrPartnerId = createQueue('rrPartnerId');
  const rrApi = {};
  const rrApiOnReady = createQueue('rrApiOnReady') || [];
  const rrApiUrl = 'https://cdn.retailrocket.net/content/javascript/tracking.js';
  rrApi.addToBasket = rrApi.order = rrApi.categoryView = rrApi.view = rrApi.recomMouseDown = rrApi.recomAddToCart = () => {};

  // Setup PartnerId
  setInWindow('rrPartnerId', data.account_id, true);
  
  // Inject API Script
  injectScript(rrApiUrl, data.gtmOnSuccess, data.gtmOnFailure, rrApiUrl);
};

const rrTrack = () => {
  switch(data.method) {
    case 'view':
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.view', data.product_id);
      });
      break;
    case 'categoryView':
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.categoryView', data.category_id);
      });
      break;
    case 'addToBasket':
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.addToBasket', data.product_id);
      });
      break;
    case 'setEmail':
      const rrOptions = {};
      if (data.custom_params) {
        data.custom_params.forEach((el) => {
          rrOptions[el.key] = el.value;
        });
      }
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.setEmail', data.user_email, rrOptions);
      });
      break;
    case 'order':
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.order', {
          transaction: data.transaction_id,
          items: data.product_array
        });
      });
      break;
    case 'search':
      callInWindow('rrApiOnReady.push', () => {
        callInWindow('rrApi.search', data.search_query);
      });
      break;     
  }
};

// Launch
switch(data.type) {
  case 'init':
    rrInit();
    break;
  case 'init_track':
    rrInit();
    rrTrack();
    break;
  case 'track':
    rrTrack();
    break;
}


